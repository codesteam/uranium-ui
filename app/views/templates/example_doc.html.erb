documentor: '1.0'
info:
  title: Dummy API
  description:
    This is example documentation for some dummy 
    photos API.
  version: 0.1 beta
paths:
  /photos:
    query: GET
    summary: Return photos array
    description: Return photos array.
    parameters:
      - name: page
        in: query
        description: Current page number
        required: no
        type: integer
      - name: page_size
        in: query
        description: Listing page size
        required: no
        type: integer
    responses:
        200:
          description: An array of photos
          type: photo_type
        default:
          description: An array of photos
          type: photo_type
  /photos/new:
    query: POST
    summary: Upload new photo.
    description: Upload new photo.
    parameters:
      - name: photo
        in: query
        description: Common photo info
        required: yes
        type: array_of_photo
    responses:
        default:
          description: An array of photos
          type: photo_type
definitions:
  photo:
    name: string
    extension: string
    demensions: photo_demensions
  photo_demensions:
    width: integer
    heigth: integer