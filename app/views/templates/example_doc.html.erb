documentor: '1.0'
info:
  title: Dummy API
  description:
    This is example documentation for some dummy 
    photos API.
  version: 0.1 beta
paths:
  /photos:
    query: GET
    summary: Return photos array
    description: Return photos array.
    parameters:
      - name: page
        description: Current page number
        required: no
        type: integer
      - name: page_size
        description: Listing page size
        required: no
        type: integer
  /photos/new:
    query: POST
    summary: Upload new photo.
    description: Upload new photo.
    parameters:
      - name: photo
        description: Serialized photo content
        required: yes
        type: string
      - name: photo_name
        description: Photo original file name
        required: yes
        type: string